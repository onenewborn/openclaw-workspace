# OpenClaw Workspace Configuration
# Copy this file to .env and fill in your values

# Cloud Service Endpoint
OPENCLAW_CLOUD_ENDPOINT=https://api.openclaw.cloud

# API Key for authentication
OPENCLAW_API_KEY=your_api_key_here

# Version file location (optional)
OPENCLAW_VERSION_FILE=.openclaw-version

# Sync interval in milliseconds (optional)
OPENCLAW_SYNC_INTERVAL=300000

# Log level (optional): debug, info, warn, error
OPENCLAW_LOG_LEVEL=info
